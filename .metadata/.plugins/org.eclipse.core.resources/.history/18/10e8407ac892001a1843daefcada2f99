package com.co.masivian.util;

/**
 * @author erick clase de declaracion de constantes para la refactorizacion del
 *         codigo
 *
 */
public class PrinterConstantes {

	static final int M = 1000;
	static final int RR = 50;
	static final int CC = 4;
	static final int ORDMAX = 30;
	static final int PAGENUMBER = 1;
	static final int PAGEOFFSET = 1;
	static final int ORD = 2;
	static final int SQUARE = 9;
	public static int getM() {
		return M;
	}
	public static int getRr() {
		return RR;
	}
	public static int getCc() {
		return CC;
	}
	public static int getOrdmax() {
		return ORDMAX;
	}
	public static int getPagenumber() {
		return PAGENUMBER;
	}
	public static int getPageoffset() {
		return PAGEOFFSET;
	}
	public static int getOrd() {
		return ORD;
	}
	public static int getSquare() {
		return SQUARE;
	}
	

}
